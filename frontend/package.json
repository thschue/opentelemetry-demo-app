    "@opentelemetry/exporter-jaeger": "^0.11.0",
    "@opentelemetry/exporter-zipkin": "^0.11.0",
    "@opentelemetry/node": "^0.11.0",
    "@opentelemetry/plugin-http": "^0.11.0",
    "@opentelemetry/tracing": "^0.11.0",
    "express": "^4.17.1"
